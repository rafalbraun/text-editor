<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkListStore" id="liststore">
    <columns>
       <column type="gchararray" />
    </columns>
      <data>
        <row>
          <col id="0" translatable="yes">General</col>
        </row>
        <row>
          <col id="0" translatable="yes">Appearance</col>
        </row>
        <row>
          <col id="0" translatable="yes">Editor</col>
        </row>
        <row>
          <col id="0" translatable="yes">Version Control</col>
        </row>
        <row>
          <col id="0" translatable="yes">Deployment</col>
