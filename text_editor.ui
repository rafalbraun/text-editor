   <object class="GtkTreeStore" id="treestore">
      <columns>
         <column type="gchararray" />
      </columns>
   </object>
   <object class="GtkListStore" id="branches">
      <columns>
         <column type="gchararray" />
      </columns>
      <data>
         <row><col id="0" translatable="yes">master</col></row>
         <row><col id="0" translatable="yes">develop</col></row>
      </data>
   </object>

   <object class="GtkMenu" id="treeview_context_menu">
                              <property name="visible">True</property>
                              <property name="can_focus">False</property>
                              <child>
                                 <object id="rename" class="GtkImageMenuItem">
